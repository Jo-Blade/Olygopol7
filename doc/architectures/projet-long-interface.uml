          <ownedAttribute xmi:id="_WTMrsPM3Ee2n1JfmXuKWRw" name="vertCode" isLeaf="true" isStatic="true" isReadOnly="true">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
            <defaultValue xmi:type="uml:LiteralString" xmi:id="_WTNSwPM3Ee2n1JfmXuKWRw" value="&quot;#version 330 core\n&quot; +&#xA;&#xA;    &quot;in vec2 vertex;\n&quot; +&#xA;    &quot;in vec2 uvs;\n&quot; +&#xA;    &quot;in vec3 positionCentre;\n&quot; +&#xA;    &quot;in float indiceTex;\n&quot; +&#xA;&#xA;    &quot;uniform int windowWidth;\n&quot; +&#xA;    &quot;uniform int windowHeight;\n&quot; +&#xA;&#xA;    &quot;uniform float isoGridWidth;\n&quot; +&#xA;    &quot;uniform float isoGridHeight;\n&quot; +&#xA;    &quot;uniform float isoGridPosX;\n&quot; +&#xA;    &quot;uniform float isoGridPosY;\n&quot; +&#xA;&#xA;    &quot;uniform float cameraX;\n&quot; +&#xA;    &quot;uniform float cameraY;\n&quot; +&#xA;    &quot;uniform float cameraZ;\n&quot; +&#xA;&#xA;    &quot;uniform float widthRatioTex;\n&quot; +&#xA;    &quot;uniform float zoom;\n&quot; +&#xA;&#xA;    &quot;out vec2 fUvs;\n&quot; +&#xA;    &quot;out float fIndiceTex;\n&quot; +&#xA;&#xA;    &quot;const float a = 9.8;\n&quot; +&#xA;    &quot;const float b = 5.6;\n&quot; +&#xA;&#xA;    &quot;void main()\n&quot; +&#xA;    &quot;{\n&quot; +&#xA;&#xA;    &quot;mat2x3 chbase = mat2x3(a, -a, 0.,   b, b, b);\n&quot; +&#xA;    &quot;vec2 pos1 = vertex*mat2(10.) + (positionCentre - vec3(cameraX, cameraY, cameraZ)) * chbase;\n&quot; +&#xA;    &quot;pos1 *= zoom;\n&quot; +&#xA;    &quot;float afficher = step(-isoGridWidth/2. + 10.*zoom, ((positionCentre - vec3(cameraX, cameraY, cameraZ)) * chbase * zoom).x);\n&quot; +&#xA;    &quot;afficher *= step(-isoGridHeight/2. + 20.*zoom, ((positionCentre - vec3(cameraX, cameraY, cameraZ)) * chbase * zoom).y);\n&quot; +&#xA;    &quot;afficher *= step(((positionCentre - vec3(cameraX, cameraY, cameraZ)) * chbase * zoom).x, isoGridWidth/2. - 10.*zoom);\n&quot; +&#xA;    &quot;afficher *= step(((positionCentre - vec3(cameraX, cameraY, cameraZ)) * chbase * zoom).y, isoGridHeight/2. - 20.*zoom);\n&quot; +&#xA;    &quot;pos1 *= afficher;\n&quot; +&#xA;&#xA;    &quot;mat2 resize = mat2(2./windowWidth, 0, 0, 2./windowHeight);\n&quot; +&#xA;    &quot;gl_Position = vec4(vec2(-1., 1.) + resize * (pos1 + vec2(isoGridPosX, - isoGridPosY) + vec2(isoGridWidth / 2., - isoGridHeight / 2.)), 0.0, 1.0);\n&quot; +&#xA;&#xA;    &quot;fUvs = uvs;\n&quot; +&#xA;    &quot;fIndiceTex = indiceTex;\n&quot; +&#xA;    &quot;}&quot;"/>
          </ownedAttribute>
