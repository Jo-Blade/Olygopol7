 drawer.addObjet(new IsoImgInstance(5,1,1,0));
 drawer.addObjet(new IsoImgInstance(6,1,1,0));
 drawer.addObjet(new IsoImgInstance(2,0,0.5f,4));
 drawer.valider();

 new Test().start();
  </body>
