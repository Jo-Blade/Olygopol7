            <defaultValue xmi:type="uml:LiteralString" xmi:id="_WS2GYPM3Ee2n1JfmXuKWRw" value="&quot;#version 330 core\n&quot; +&#xA;&#xA;    &quot;in vec2 position;\n&quot; +&#xA;    &quot;in vec2 uvs;\n&quot; +&#xA;    &quot;in vec2 point1;\n&quot; +&#xA;    &quot;in vec2 dimensions;\n&quot; +&#xA;    &quot;in vec4 couleurBordure;\n&quot; +&#xA;    &quot;in vec4 couleurFond;\n&quot; +&#xA;    &quot;in float rayon;\n&quot; +&#xA;    &quot;in float epaisseur;\n&quot; +&#xA;&#xA;    &quot;uniform int windowWidth;\n&quot; +&#xA;    &quot;uniform int windowHeight;\n&quot; +&#xA;&#xA;    &quot;out vec2 fragUvs;\n&quot; +&#xA;&#xA;    &quot;out float width;\n&quot; +&#xA;    &quot;out float height;\n&quot; +&#xA;    &quot;out vec4 border;\n&quot; +&#xA;    &quot;out vec4 fond;\n&quot; +&#xA;    &quot;out float radius;\n&quot; +&#xA;    &quot;out float border_size;\n&quot; +&#xA;&#xA;    &quot;void main()\n&quot; +&#xA;    &quot;{\n&quot; +&#xA;&#xA;    &quot;mat2 resize = 2*mat2(dimensions.x / float(windowWidth), 0.0, 0.0, dimensions.y / float(windowHeight));\n&quot; +&#xA;&#xA;    &quot;gl_Position = vec4(vec2(-1., 1.) + position*resize + vec2(2. / float(windowWidth), -2. / float(windowHeight))*point1, 0.0, 1.0);\n&quot; +&#xA;    &quot;fragUvs = uvs;\n&quot; +&#xA;&#xA;    &quot;width = dimensions.x;\n&quot; +&#xA;    &quot;height = dimensions.y;\n&quot; +&#xA;    &quot;border = couleurBordure;\n&quot; +&#xA;    &quot;fond = couleurFond;\n&quot; +&#xA;    &quot;radius = rayon;\n&quot; +&#xA;    &quot;border_size = epaisseur;\n&quot; +&#xA;    &quot;}&quot;"/>
          </ownedAttribute>
          <ownedAttribute xmi:id="_WS2tcfM3Ee2n1JfmXuKWRw" name="fragCode" visibility="private" isLeaf="true" isStatic="true" isReadOnly="true">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
            <defaultValue xmi:type="uml:LiteralString" xmi:id="_WS3UgPM3Ee2n1JfmXuKWRw" value="&quot;#version 330 core\n&quot; +&#xA;&#xA;    &quot;out vec4 fragColor;\n&quot; +&#xA;&#xA;    &quot;in vec2 fragUvs;\n&quot; +&#xA;&#xA;    &quot;in float width;\n&quot; +&#xA;    &quot;in float height;\n&quot; +&#xA;    &quot;in vec4 border;\n&quot; +&#xA;    &quot;in vec4 fond;\n&quot; +&#xA;    &quot;in float radius;\n&quot; +&#xA;    &quot;in float border_size;\n&quot; +&#xA;&#xA;    &quot;uniform int windowWidth;\n&quot; +&#xA;    &quot;uniform int windowHeight;\n&quot; +&#xA;&#xA;    &quot;void main() {\n&quot; +&#xA;    &quot;vec2 st = abs(fragUvs);\n&quot; +&#xA;&#xA;    &quot;st.x *= width;\n&quot; +&#xA;    &quot;st.y *= height;\n&quot; +&#xA;&#xA;    &quot;float lg = length(st - vec2(width/2. - radius, height/2. - radius));\n&quot; +&#xA;    // &quot;float aa = max(6/windowWidth, 6/windowHeight);\n&quot; +&#xA;    // &quot;float aa = 1.5;\n&quot; +&#xA;    // &quot;float aa = 0;\n&quot; +&#xA;&#xA;    &quot;float alpha = smoothstep(lg - 1, lg + 1, radius);\n&quot; +&#xA;    &quot;alpha += step(radius, length(st - vec2(width/2.,height/2.)));\n&quot; +&#xA;&#xA;    &quot;float radius2 = radius - border_size;\n&quot; +&#xA;    &quot;float choisir = smoothstep(radius2 - 1.5, radius2, lg);\n&quot; +&#xA;&#xA;    &quot;vec2 d = vec2(width/2., height/2.) - st;\n&quot; +&#xA;    &quot;choisir -= step(border_size + 1, d.x)*step(radius2, d.y) + step(radius2, d.x)*step(border_size + 1, d.y);\n&quot; +&#xA;    &quot;choisir = max(choisir, 0.);\n&quot; +&#xA;&#xA;    &quot;vec4 color = border*choisir + (1. - choisir) * fond;\n&quot; +&#xA;    &quot;fragColor = vec4(color.rgb, min(color.a, alpha));\n&quot; +&#xA;    &quot;}\n&quot;"/>
