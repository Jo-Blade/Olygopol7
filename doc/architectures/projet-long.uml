     // IL FAUT CHANGER LE NULL PAR LE TABLEAU DES JOUEURS !!!
 	  CaseLibre newCaseLibre = new CaseLibre(i, 10, this, testScan, null);
       listeCaseFonctionnelle.add(newCaseLibre);
       listeCaseGraphique.add(newCaseLibre);
 	}

 }
  </body>
