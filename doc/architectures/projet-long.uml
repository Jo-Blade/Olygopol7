          <ownedAttribute xmi:id="_3W8EUPM1Ee2n1JfmXuKWRw" name="vertCode" isLeaf="true" isStatic="true" isReadOnly="true">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
            <defaultValue xmi:type="uml:LiteralString" xmi:id="_3W8rYPM1Ee2n1JfmXuKWRw" value="&quot;#version 330 core\n&quot; +&#xA;&#xA;    &quot;in vec2 position;\n&quot; +&#xA;    &quot;in vec2 uvs;\n&quot; +&#xA;    &quot;in vec2 point1;\n&quot; +&#xA;    &quot;in vec2 dimensions;\n&quot; +&#xA;&#xA;    &quot;uniform int windowWidth;\n&quot; +&#xA;    &quot;uniform int windowHeight;\n&quot; +&#xA;&#xA;    &quot;out vec2 fragUvs;\n&quot; +&#xA;&#xA;    &quot;void main()\n&quot; +&#xA;    &quot;{\n&quot; +&#xA;&#xA;    &quot;mat2 resize = 2*mat2(dimensions.x / float(windowWidth), 0.0, 0.0, dimensions.y / float(windowHeight));\n&quot; +&#xA;&#xA;    &quot;gl_Position = vec4(vec2(-1., 1.) + position*resize + vec2(2. / float(windowWidth), -2. / float(windowHeight))*point1, 0.0, 1.0);\n&quot; +&#xA;    &quot;fragUvs = uvs;\n&quot; +&#xA;    &quot;}&quot;"/>
          </ownedAttribute>
          <ownedAttribute xmi:id="_3W9ScfM1Ee2n1JfmXuKWRw" name="fragCode" visibility="private" isLeaf="true" isStatic="true" isReadOnly="true">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
            <defaultValue xmi:type="uml:LiteralString" xmi:id="_3W95gPM1Ee2n1JfmXuKWRw" value="&quot;#version 330 core\n&quot; +&#xA;&#xA;    &quot;out vec4 fragColor;\n&quot; +&#xA;&#xA;    &quot;in vec2 fragUvs;\n&quot; +&#xA;    &quot;uniform sampler2D textureSampler;\n&quot; +&#xA;&#xA;    &quot;void main() {\n&quot; +&#xA;    &quot;fragColor = texture(textureSampler, fragUvs);\n&quot; +&#xA;    &quot;}\n&quot;"/>
          </ownedAttribute>
