            <defaultValue xmi:type="uml:LiteralString" xmi:id="_3WszwPM1Ee2n1JfmXuKWRw" value="&quot;#version 330 core\n&quot; +&#xA;&#xA;    &quot;out vec4 fragColor;\n&quot; +&#xA;&#xA;    &quot;in vec2 fragUvs;\n&quot; +&#xA;&#xA;    &quot;in float width;\n&quot; +&#xA;    &quot;in float height;\n&quot; +&#xA;    &quot;in vec4 border;\n&quot; +&#xA;    &quot;in vec4 fond;\n&quot; +&#xA;    &quot;in float radius;\n&quot; +&#xA;    &quot;in float border_size;\n&quot; +&#xA;&#xA;    &quot;uniform int windowWidth;\n&quot; +&#xA;    &quot;uniform int windowHeight;\n&quot; +&#xA;&#xA;    &quot;void main() {\n&quot; +&#xA;    &quot;vec2 st = abs(fragUvs);\n&quot; +&#xA;&#xA;    &quot;st.x *= width;\n&quot; +&#xA;    &quot;st.y *= height;\n&quot; +&#xA;&#xA;    &quot;float lg = length(st - vec2(width/2. - radius, height/2. - radius));\n&quot; +&#xA;    // &quot;float aa = max(6/windowWidth, 6/windowHeight);\n&quot; +&#xA;    // &quot;float aa = 1.5;\n&quot; +&#xA;    // &quot;float aa = 0;\n&quot; +&#xA;&#xA;    &quot;float alpha = smoothstep(lg - 1, lg + 1, radius);\n&quot; +&#xA;    &quot;alpha += step(radius, length(st - vec2(width/2.,height/2.)));\n&quot; +&#xA;&#xA;    &quot;float radius2 = radius - border_size;\n&quot; +&#xA;    &quot;float choisir = smoothstep(radius2 - 1.5, radius2, lg);\n&quot; +&#xA;&#xA;    &quot;vec2 d = vec2(width/2., height/2.) - st;\n&quot; +&#xA;    &quot;choisir -= step(border_size + 1, d.x)*step(radius2, d.y) + step(radius2, d.x)*step(border_size + 1, d.y);\n&quot; +&#xA;    &quot;choisir = max(choisir, 0.);\n&quot; +&#xA;&#xA;    &quot;vec4 color = border*choisir + (1. - choisir) * fond;\n&quot; +&#xA;    &quot;fragColor = vec4(color.rgb, min(color.a, alpha));\n&quot; +&#xA;    &quot;}\n&quot;"/>
