            <defaultValue xmi:type="uml:LiteralString" xmi:id="_3XBj4PM1Ee2n1JfmXuKWRw" value="new float[]&#xA;  {&#xA;    +0.0f, +0.0f,    // Top-left coordinate&#xA;      +0.0f, +1.0f,    // Bottom-left coordinate&#xA;      +1.0f, +1.0f,    // Bottom-right coordinate&#xA;&#xA;      +1.0f, +0.0f,    // Top-right&#xA;      +0.0f, +0.0f,    // Top-left&#xA;      +1.0f, +1.0f     // Bottom-right&#xA;  }"/>
