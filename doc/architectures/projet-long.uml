            <defaultValue xmi:type="uml:LiteralString" xmi:id="_3XZXUPM1Ee2n1JfmXuKWRw" value="&quot;#version 330 core\n&quot; +&#xA;&#xA;    &quot;in vec2 vertex;\n&quot; +&#xA;    &quot;in vec2 uvs;\n&quot; +&#xA;    &quot;in vec2 positionLettre;\n&quot; +&#xA;    &quot;in vec2 glyphPos;\n&quot; +&#xA;    &quot;in vec2 glyphTaille;\n&quot; +&#xA;    &quot;in float echelle;\n&quot; +&#xA;    &quot;in vec4 couleur;\n&quot; +&#xA;&#xA;    &quot;uniform int windowWidth;\n&quot; +&#xA;    &quot;uniform int windowHeight;\n&quot; +&#xA;&#xA;    &quot;out vec2 fUvs;\n&quot; +&#xA;    &quot;out vec2 fGlyphPos;\n&quot; +&#xA;    &quot;out vec2 fGlyphTaille;\n&quot; +&#xA;    &quot;out vec4 fCouleur;\n&quot; +&#xA;&#xA;    &quot;void main()\n&quot; +&#xA;    &quot;{\n&quot; +&#xA;&#xA;    &quot;mat2 resize = echelle * mat2(50. / float(windowWidth), 0.0, 0.0, (50. * glyphTaille.y) / (float(windowHeight) * glyphTaille.x));\n&quot; +&#xA;    &quot;mat2 chbase = mat2(2. / float(windowWidth), 0., 0., - 2. / float(windowHeight));\n&quot; +&#xA;    &quot;gl_Position = vec4(resize*vertex + chbase*positionLettre + vec2(-1.0, 1.0), 0.0, 1.0);\n&quot; +&#xA;&#xA;    &quot;fUvs = uvs;\n&quot; +&#xA;    &quot;fGlyphPos = glyphPos;\n&quot; +&#xA;    &quot;fGlyphTaille = glyphTaille;\n&quot; +&#xA;    &quot;fCouleur = couleur;\n&quot; +&#xA;&#xA;    &quot;}&quot;"/>
