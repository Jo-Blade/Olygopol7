          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_3aeggfM1Ee2n1JfmXuKWRw" name="creerTexture" specification="_3DlqgPM1Ee2n1JfmXuKWRw">
            <language>java</language>
            <body>ByteBuffer buffer;

 try (MemoryStack stack = MemoryStack.stackPush()){
   IntBuffer w = stack.mallocInt(1);
   IntBuffer h = stack.mallocInt(1);
   IntBuffer channels = stack.mallocInt(1);

   File file = new File(&quot;res/&quot;+resourceName);
   String filePath = file.getAbsolutePath();
   buffer = STBImage.stbi_load(filePath, w, h, channels, 4);
   if(buffer ==null) {
     throw new Exception(&quot;Can't load file &quot; + resourceName + &quot; &quot;
         + STBImage.stbi_failure_reason());
   }

   uploadImageBuffer(buffer, w.get(), h.get());

 } catch(Exception e) {
   e.printStackTrace();
 }

  </body>
          </ownedBehavior>
