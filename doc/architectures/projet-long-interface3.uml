      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_GbdsEPM5Ee2n1JfmXuKWRw" name="main" specification="_GMgqEPM5Ee2n1JfmXuKWRw">
        <ownedParameter xmi:id="_GbdsEfM5Ee2n1JfmXuKWRw" name="args">
          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
